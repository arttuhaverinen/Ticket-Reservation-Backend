{
	"name": "ticket-reservation-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"test": "vitest",
		"testmod": "vite --mode test --port 3000 & npx cypress run",
		"test:cypress": "npx cypress run",
